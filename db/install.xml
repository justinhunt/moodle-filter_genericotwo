<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="filter/genericotwo/db" VERSION="2025062100" COMMENT="Install table for genericotwo templates">
    <TABLES>
        <TABLE NAME="filter_genericotwo_templates" COMMENT="Store genericotwo templates">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="version" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="templatekey" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="customcss" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="variables" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="content" TYPE="text" NOTNULL="true"/>
                <FIELD NAME="jscontent" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="previewcontext" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>